\mode<presentation>

\definecolor{ulsterdarkorange}{rgb}{0.839, 0.369, 0.235}
\definecolor{ulsterlightorange}{rgb}{0.945, 0.753, 0.604}

% Settings
\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{subtitle page header}{fg=ulsterdarkorange}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{date}{fg=white}

\setbeamercolor*{item}{fg=ulsterdarkorange}
\setbeamercolor*{itemize item}{fg=ulsterdarkorange}
\setbeamercolor*{enumerate item}{fg=ulsterdarkorange}
\setbeamercolor*{block title}{bg=ulsterdarkorange,fg=white}
\setbeamercolor*{block body}{bg=ulsterlightorange,fg=black}
\setbeamercolor*{alerted text}{fg=ulsterdarkorange}
\setbeamercolor*{section in toc}{fg=ulsterdarkorange}



\mode
<all>